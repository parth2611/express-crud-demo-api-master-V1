<%- include('header'); -%>

 
<form method="post" enctype="multipart/form-data">

    <table>

        <tr>
            <td>Name : </td>
            <td>
                <input type="text" placeholder="Enter Name" name="user_name" value="<%= users.user_name %>" required> </td>
        </tr>
        <tr>
            <td> Email :</td>
            <td>
                <input type="text" placeholder="Enter Email" name="user_email" value="<%= users.user_email %>" required>
            </td>
        </tr>
        <tr>
            <td>Mobile :</td>
            <td>
                <input type="text" placeholder="Enter Mobile" name="user_mobile" value="<%= users.user_mobile %>" required>
            </td>
        </tr>

        value=""
        <tr>
            <td>Mobile :</td>
            <td>
                <img style="width:100px;" src="../upload/<%= users.user_photo %>">
            </td>
        </tr>

        <tr>
            <td>File :</td>
            <td>
                <input type="file" placeholder="Enter Mobile" name="user_photo"  required>
            </td>
        </tr>
        <tr>

            <td>
                <input type="submit">
                  <input type="button" value="Display" onclick="window.location='display';">
            </td>
        </tr>
    </table>





</form>

 